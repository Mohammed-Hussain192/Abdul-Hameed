                    <div class="education-location">
                        <i class="fas fa-map-marker-alt"></i>
                        <span>Bangalore, Karnataka</span>
                    </div>
                </div>
                
                <!-- Engineering -->
                <div class="education-card fade-in delay-3">
                    <div class="education-icon">
                        <i class="fas fa-laptop-code"></i>
                    </div>
                    <span class="education-year">2022 - Present</span>
                    <h3 class="education-title">Engineering (BE)</h3>
                    <p class="education-institution">T John Institute of Technology</p>
                    <div class="education-location">
                        <i class="fas fa-map-marker-alt"></i>
                        <span>Bangalore, Karnataka</span>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Skills Section -->
    <section class="skills-section">
        <div class="container">
            <div class="section-header">
                <h2 class="section-title fade-in">My Skills</h2>
                <p class="section-subtitle fade-in delay-1">Technical expertise and professional capabilities</p>
            </div>
            
            <div class="skills-container">
                <!-- Technical Skills -->
                <div class="skill-card fade-in delay-1">
                    <h3><i class="fas fa-code"></i> Technical Skills</h3>
                    <div class="skills-grid">
                        <div class="skill-item">Python</div>
                        <div class="skill-item">JavaScript</div>
                        <div class="skill-item">HTML & CSS</div>
                        <div class="skill-item">Node.js & Express</div>
                        <div class="skill-item">MongoDB</div>
                        <div class="skill-item">Machine Learning</div>
                        <div class="skill-item">Artificial Intelligence</div>
                        <div class="skill-item">Data Structures & Algorithms</div>
                        <div class="skill-item">REST APIs</div>
                        <div class="skill-item">Computer Vision</div>
                        <div class="skill-item">Git & GitHub</div>
                        <div class="skill-item">React</div>
                    </div>
                </div>
                
                <!-- Soft Skills -->
                <div class="skill-card fade-in delay-2">
